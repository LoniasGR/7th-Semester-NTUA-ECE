IN 10H
L1:
CALL KIND   ; loads the code in A
MOV C,A     ; temporary
ANI 0FH     ; keep LSB	
LXI H,0860H
MOV M,A     ; store A in memory
MOV A,C
ANI F0H     ; keep ms byte
RRC 
RRC
RRC
RRC         ; formatting
INX H       ; next 7-segment
MOV M,A     ; store A in memory
LXI D,0860H ; in limits
CALL STDM
CALL DCD
JMP L1
END